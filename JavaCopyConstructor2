class Student
{   int id;
    String name;
    Student(int i,String n)
    {
        id=i;
        name=n;
    }
    
    Student()
    {
        
    }
    void display()
    {
        System.out.println("ID: "+id);
        System.out.println("Name:"+ name);
    }
}
public class Test
{
    public static void main(String[] args)
    {
        Student s1=new Student(1,"Shrawan");
        Student s2=new Student();
        
        s2.id=s1.id;
        s2.name=s1.name;
        s1.display();
        s2.display();
    }
}
