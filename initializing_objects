class Student
{
  int id;
  String name;
  void setObjectVariables(int i,String n)
    {
        id=i;
        name=n;
    }
    
    void displayInfo()
    {
        
        System.out.println(id+" "+name);
    }
}

public class Test1
{
  public static void main(String[] args)
  {
    Student s1=new Student();
    //Intializing the object through reference variable
    s1.id=90;
    s1.name="Shrawan";
    System.out.println(s1.id+" "+s1.name);
    
    
    //Initializing the object through method
    Student s2=new Student();
    s2.setObjectVariables(4,"Rani");
    s2.displayInfo();
 }
}
